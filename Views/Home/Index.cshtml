@using RunningApp
@model IEnumerable<RunningInfo>
<div
  class="relative pt-16 pb-32 flex content-center items-center justify-center"
  style="min-height: 75vh"
>
  <div
    class="absolute top-0 w-full h-full bg-center bg-cover"
    style="
      background-image: url('assets/img/gabin-vallet-J154nEkpzlQ-unsplash.jpg');
    "
  >
    <span
      id="blackOverlay"
      class="w-full h-full absolute opacity-75 bg-black"
    ></span>
  </div>
  <div class="container relative mx-auto">
    <div class="items-center flex flex-wrap">
      <div class="w-full lg:w-6/12 px-4 ml-auto mr-auto text-center">
        <div class="pr-12">
          <h1 class="text-white font-semibold text-5xl">Ska vi springa?</h1>
          <p class="mt-4 text-lg text-gray-300">
            "Springa är ett enkelt sätt att boosta både kroppen och humöret.
            Hitta en ny löparkompis och känn hur motivationen höjs och stegen lättar."
          </p>
        </div>
        <div>
          <h3 class="text-white font-semibold text-3xl mt-10">Skapa en löptur eller joina en befintlig under</h3>
        </div>
      </div>
    </div>
  </div>
  <div
    class="top-auto bottom-0 left-0 right-0 w-full absolute pointer-events-none overflow-hidden"
    style="height: 70px"
  >
    <svg
      class="absolute bottom-0 overflow-hidden"
      xmlns="http://www.w3.org/2000/svg"
      preserveAspectRatio="none"
      version="1.1"
      viewBox="0 0 2560 100"
      x="0"
      y="0"
    >
      <polygon
        class="text-gray-300 fill-current"
        points="2560 0 2560 100 0 100"
      ></polygon>
    </svg>
  </div>
</div>
<section class="pb-20 bg-gray-300 -mt-24">
  <div class="container mx-auto px-4">
    <div class="flex justify-center">
      <!-- adsf -->
      <div class="w-full md:w-6/12 px-4 text-center">
        <div
          id="load-run"
          class="relative flex flex-col min-w-0 break-words bg-white w-full mb-8 shadow-lg rounded-lg hidden"
        >
          <div class="px-4 p-5 flex-auto">
            <h6 class="text-xl font-semibold">Letar löpare!</h6>
            <video src="/assets/run1.mp4" muted autoplay loop></video>
          </div>
        </div>
        <div
          id="prompt-run"
          class="relative flex flex-col min-w-0 break-words bg-white w-full mb-8 shadow-lg rounded-lg"
        >
          <div class="px-4 p-5 flex-auto">
            <div
              class="text-white p-3 text-center inline-flex items-center justify-center w-12 h-12 mb-5 shadow-lg rounded-full bg-blue-400"
            >
              <i class="fas fa-running"></i>
            </div>
            <h6 class="text-xl font-semibold">Hitta någon att springa med!</h6>

          </div>
          @using (Html.BeginForm("GetRunningInfo", "Home", FormMethod.Post)) 
          {
          <div class="px-4 pt-4 pb-5 flex flex-col">

            <!-- Choose time -->
            <div class="flex items-start flex-col px-4 pb-2">
              <label
                for="startTime"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >När?</label
              >
              <input
                type="datetime-local"
                value="2023-05-12T19:30"
                placeholder="Närsomhelst"
                id="startTime"
                name="startTime"
                class="block w-full p-2 text-gray-900 border border-gray-300 rounded-lg bg-gray-50 sm:text-xs focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
              />
            </div>

            <!-- Choose track -->
            <div class="flex items-start flex-col px-4 pb-4">
              <label
                for="where-input"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Var?</label
              >
              <select class="block w-full p-2 text-gray-900 border border-gray-300 rounded-lg bg-gray-50 sm:text-xs focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" 
                  id="track" 
                  name="track">
                <option>Välj elljusspår</option> 
              </select>
            </div>

            <!-- Choose time -->
            <div class="flex items-start flex-col px-4 pb-2">
              <label
                for="tempo"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Tempo?</label
              >
              <input
                type="number"
                value=""
                placeholder="tempo i km/h"
                id="tempo"
                name="tempo"
                class="block w-full p-2 text-gray-900 border border-gray-300 rounded-lg bg-gray-50 sm:text-xs focus:ring-blue-500 focus:border-blue-500 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
              />
            </div>

            <!-- Choose time -->
            <input 
              value="0"
              type="hidden" 
              id="userId"
              name="userId"
              >


            <div class="flex items-start flex-col px-4 pb-4">
              <button
                id="toggle-create-run"
                disabled
                class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mt-5">
                Skapa löprunda?
                <span class="fas fa-running"></span>
              </button>
            </div>
          </div>
          }
        </div>
      </div>
    </div>

      
    <div class="flex flex-wrap items-center mt-16">
      <div class="w-full  px-4 mr-auto ml-auto">
        <div
          class="text-gray-600  p-3 text-center inline-flex items-center justify-center w-16 h-16 mb-6 shadow-lg rounded-full bg-gray-100"
        >
          <i class="fas fa-user-friends text-xl"></i>
        </div>
        <h3 class="text-3xl mb-2 font-semibold leading-normal">
          Uppkommande springturer
        </h3>
                <div
          class="relative flex flex-col p-4 w-0 break-words bg-white w-full mb-8 shadow-lg rounded-lg"
        >
          <table class="table-fixed">
            <thead>
              <tr>
                <td>Datum</td>
                <td>Bana</td>
                <td>Tempo</td>
                <td>Smeknamn</td>
                <td></td>
              </tr>
            </thead>
            <tbody>
            @foreach(RunningInfo info in Model)
            {
              <tr>
                <td>@info.StartTime</td>
                <td>@info.Track</td>
                <td>~@info.Tempo km/h</td>
                <td>@info.User.NickName</td>
                <td>
                  <div class="join bg-blue-500 cursor-pointer hover:bg-blue-700 text-white text-center font-bold py-2 px-4 rounded">
join</div>
                </td>
              </tr>
            }
            </tbody>
          </table>
      </div>

          <div class="flex flex-wrap items-center mt-16">
      </div>
      <div class="w-full px-4 mr-auto ml-auto">
          <div id="map" style="width:100%; height:600px;max-height:60vh;">
          </div>
      </div>
    </div>
      </div>

      
